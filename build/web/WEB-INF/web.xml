<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PesquisaAlunoController</servlet-name>
        <servlet-class>controller.PesquisaAlunoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaGerenteController</servlet-name>
        <servlet-class>controller.PesquisaGerenteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaAlbumController</servlet-name>
        <servlet-class>controller.PesquisaAlbumController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaAdministradorController</servlet-name>
        <servlet-class>controller.PesquisaAdministradorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaCertificadoController</servlet-name>
        <servlet-class>controller.PesquisaCertificadoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaEventoController</servlet-name>
        <servlet-class>controller.PesquisaEventoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaExternoController</servlet-name>
        <servlet-class>controller.PesquisaExternoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaGaleriaController</servlet-name>
        <servlet-class>controller.PesquisaGaleriaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaLocalController</servlet-name>
        <servlet-class>controller.PesquisaLocalController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaNoticiaController</servlet-name>
        <servlet-class>controller.PesquisaNoticiaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaPropostaController</servlet-name>
        <servlet-class>controller.PesquisaPropostaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TratamentoExcecao</servlet-name>
        <servlet-class>exception.TratamentoExcecao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisaAlunoController</servlet-name>
        <url-pattern>/PesquisaAlunoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaGerenteController</servlet-name>
        <url-pattern>/PesquisaGerenteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaAlbumController</servlet-name>
        <url-pattern>/PesquisaAlbumController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaAdministradorController</servlet-name>
        <url-pattern>/PesquisaAdministradorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaCertificadoController</servlet-name>
        <url-pattern>/PesquisaCertificadoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaEventoController</servlet-name>
        <url-pattern>/PesquisaEventoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaExternoController</servlet-name>
        <url-pattern>/PesquisaExternoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaGaleriaController</servlet-name>
        <url-pattern>/PesquisaGaleriaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaLocalController</servlet-name>
        <url-pattern>/PesquisaLocalController</url-pattern>
